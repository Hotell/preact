{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"lib": [
			"es6",
			"dom"
		],
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"baseUrl": "./",
		"typeRoots": [
			"../../"
		],
		"types": ["devtools"],
		"paths": {
			"preact": ["../../src/preact.d.ts"]
		},
		"noEmit": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react",
		"jsxFactory": "h",
		"pretty": true
	},
	"files": [
		"prect-test.tsx",
		"../../src/preact.d.ts"
	]
}
